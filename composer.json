{
    "name": "wujunze/laravel-debug-helper",
    "description": "Laravel package to help debug",
    "type": "library",
    "keywords": [
        "Laravel",
        "debug-helper",
        "sql",
        "logging"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "wujunze",
            "email": "itwujunze@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "illuminate/support": "5.*",
        "laravel/framework": "5.*"
    },
    "autoload": {
        "psr-4": {
            "WuJunze\\LaravelDebugHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WuJunze\\LaravelDebugHelper\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^7.4",
        "codedungeon/phpunit-result-printer": "^0.23.3"
    }
}
